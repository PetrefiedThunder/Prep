{
  "name": "prepchef",
  "version": "0.1.0",
  "private": true,
  "description": "PrepChef monorepo workspace",
  "packageManager": "npm@10.8.1",
  "engines": {
    "node": ">=20.11.0"
  },
  "workspaces": [
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "build": "npm run tsconfig:sync && npm run codegen && npm -ws --if-present run build",
    "dev": "npm run tsconfig:sync && npm -ws --if-present run dev",
    "start": "npm -ws --if-present run start",
    "lint": "npm -ws --if-present run lint",
    "test": "npm -ws --if-present run test",
    "typecheck": "npm -ws --if-present run typecheck",
    "codegen": "node scripts/codegen.js",
    "tsconfig:sync": "node scripts/sync-tsconfig.mjs",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:create": "prisma migrate dev --create-only",
    "prisma:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "lucide-react": "^0.553.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.929.0",
    "@fastify/multipart": "^9.3.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "openapi-typescript": "^7.10.1",
    "prisma": "^5.20.0",
    "stripe": "^20.0.0",
    "typescript": "^5.6.3"
  }
}