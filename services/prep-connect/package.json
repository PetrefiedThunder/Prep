{
  "name": "@prep/prep-connect",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "jest",
    "build": "tsc --project tsconfig.json && node dist/scripts/generate-openapi.js",
    "start": "node dist/index.js",
    "generate:openapi": "ts-node src/scripts/generate-openapi.ts",
    "deploy": "node dist/scripts/deploy.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "openapi3-ts": "^4.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^30.2.0",
    "prettier": "^3.2.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
